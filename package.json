{
  "name": "lia-sdk-core",
  "version": "0.1.0",
  "description": "Lithium Community Core JavaScript SDK",
  "main": "lib/library.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adityasharat/lia-sdk-core.git"
  },
  "keywords": [
    "lithium",
    "lia",
    "community",
    "core",
    "sdk",
    "library"
  ],
  "author": "Aditya Sharat",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adityasharat/lia-sdk-core/issues"
  },
  "homepage": "https://github.com/adityasharat/lia-sdk-core",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "mocha": "^4.0.1",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "axios": "^0.18.0"
  }
}
